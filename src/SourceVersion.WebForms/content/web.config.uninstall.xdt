<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<system.webServer>
		<handlers>
			<add 
        name="Version"
        xdt:Transform="Remove"
        xdt:Locator="Match(name)" />
		</handlers>
    <handlers 
      xdt:Locator="Condition(count(*) = 0)"
      xdt:Transform="Remove"/>
	</system.webServer>
</configuration>